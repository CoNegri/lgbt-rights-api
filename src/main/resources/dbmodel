SELECT   TB_RIGHTS_PER_COUNTRY

RIGHTS
SSMG | SAME-SEX-MARRIAGE


SELECT 
JOIN TB_SAME_SEX_MARRIAGE_STATUS


	
	
	SELECT 
	
	SELECT  TBRPC.COUNTRY, 
			TBCY.NAME,
			TBCY.CONTINENT,
			
	
	FROM TBRPC 
	JOIN TBRS 
	ON TBRPC.RIGHT_STATUS = TBRS.CODE
	JOIN TBCY
	ON TBRPC.COUNTRY = TBCY.CODE
	
	


	TBRS
	CODE DESCRIPTION
	M1
	
	TBRPC
	COUNTRY*	|	RIGHT* 	| YEAROFIMPLEMENTATION | OBSERVATION	| LEGAL | RIGHT_STATUS
	TWN			|   010		| 1920				   |				| true 	| 
	
	

